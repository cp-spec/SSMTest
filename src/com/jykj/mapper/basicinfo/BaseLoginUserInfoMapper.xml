<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jykj.mapper.basicinfo.BaseLoginUserInfoMapper">

	<select id="selectUserInfo" resultType="map">
		select t.user_id,t.user_name from base_login_user_info t where t.user_id between 880 and 889
	</select>

</mapper>